{
  "name": "wellbe-full",
  "scripts": {
    "install": "yarn build-all",
    "next": "yarn build-firebase && cd \"client\" && yarn && yarn dev",
    "preserve": "yarn build-all",
    "serve": "firebase serve",
    "predeploy": "yarn build-all",
    "deploy": "firebase deploy",
    "build-all": "yarn build-next && yarn build-firebase",
    "build-next": "cd \"client\" && yarn && yarn build",
    "build-firebase": "cd \"server/functions\" && yarn",
    "lint": "cd \"client\" && yarn next lint"
  },
  "engines": {
    "node": "18"
  },
  "main": "dist/server/index.js",
  "dependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.11",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.7.0",
    "next": "^14.1.0",
    "next-firebase": "^1.1.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^5.1.3",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^4.9.0"
  },
  "private": true
}
